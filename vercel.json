{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/icons/(.*)", "dest": "/icons/$1" },
    { "src": "/screenshots/(.*)", "dest": "/screenshots/$1" },
    { "src": "/registerSW.js", "dest": "/registerSW.js" },
    { "src": "/manifest.webmanifest", "dest": "/manifest.webmanifest" },
    { "src": "/logo.png", "dest": "/logo.png" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/site.webmanifest", "dest": "/site.webmanifest" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
